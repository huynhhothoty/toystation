{
    "builds": [{ "src": "*.js", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
